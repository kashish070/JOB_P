<%- include("./header.ejs") %> <%- include("./sidebar.ejs") %>

<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0">List of jobseeker</h1>
        </div>
        <!-- /.col -->
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="#">Home</a></li>
            <li class="breadcrumb-item active">Display</li>
          </ol>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </div>
  <!-- /.content-header -->
  <!-- </div> -->
  <div class="card">
    <div class="card-header">
      <!-- <h3 class="card-title">Category Display</h3> -->
    </div>
    <!-- /.card-header -->
    <div class="card-body">
      <table id="example1" class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Image</th>
            <th>Action</th>
            <!-- <th>Action</th> -->
          </tr>
        </thead>
        <tbody>
          <% e.forEach((item)=>{%>

          <tr>
            <td><%= item.id %></td>
            <td><%= item.name %></td>
            <td>
              <img src="<%= item.image.url %>" width="50" height="50" alt="" />
            </td>

            <td>
              <a href="/admin/category/view/<%= item.id %>" class="btn btn-info"
                ><i class="fa-solid fa-eye"></i
              ></a>

              <a
                href="/admin/category/edit/<%= item.id %>"
                class="btn btn-success"
                ><i class="fa-solid fa-pen-to-square"></i
              ></a>

              <a
                href="/admin/category/delete/<%= item.id %>"
                class="btn btn-danger"
                ><i class="fa-solid fa-trash"></i
              ></a>
            </td>
          </tr>

          <% }) %>
        </tbody>
        <!-- <tfoot>
          <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Image</th>
            <th></th>
          </tr>
        </tfoot> -->
      </table>
    </div>
    <!-- /.card-body -->
  </div>
  <!-- /.card -->
  <!-- /.card -->/
</div>
<!-- /.content-wrapper -->

<%- include("./footer.ejs") %>
